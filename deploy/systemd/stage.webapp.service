[Unit]
Description=GarPr Stage Webapp service

[Service]
ExecStart=/usr/local/bin/twistd \
    --nodaemon \
    --pidfile= \
    --logfile="logs/webapp.log" \
    -oy webapp/serve_webapp.tac

WorkingDirectory=/srv/garpr/stage

User=root
Group=root

Restart=always

[Install]
WantedBy=multi-user.target
